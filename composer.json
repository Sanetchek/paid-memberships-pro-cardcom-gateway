{
  "name": "sanetchek/pmpro-cardcom",
  "type": "wordpress-plugin",
  "version": "1.0.0",
  "description": "Payment gateway integration for PMPro using Cardcom",
  "keywords": [
    "WordPress",
    "Plugin",
    "PMPro",
    "Cardcom"
  ],
  "homepage": "https://github.com/Sanetchek/pmpro-cardcom",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Contributors",
      "homepage": "https://github.com/Sanetchek/pmpro-cardcom/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=5.6",
    "mpdf/mpdf": "^8.1",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
    "wptrt/wpthemereview": "^0.2.1",
    "php-parallel-lint/php-parallel-lint": "^1.2.0",
    "wp-cli/i18n-command": "^2.2.5"
  },
  "scripts": {
    "lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
    "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
    "make-pot": "wp i18n make-pot . languages/pmpro-cardcom.pot"
  },
  "support": {
    "issues": "https://github.com/Sanetchek/pmpro-cardcom/issues",
    "source": "https://github.com/Sanetchek/pmpro-cardcom"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
